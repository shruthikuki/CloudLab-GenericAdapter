<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>cloudlab.adaptertrial.JSONRpcServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenericAdapter</servlet-name>
        <servlet-class>cloudlab.genericadapter.GenericOpsAdapter</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenericAdapter</servlet-name>
        <url-pattern>/genericOps</url-pattern>
    </servlet-mapping>
</web-app>

